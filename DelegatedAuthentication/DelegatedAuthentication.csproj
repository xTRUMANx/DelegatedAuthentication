<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>Delegated Authentication</Title>
    <Authors>Mustafa Saeed Haji Ali</Authors>
    <Description>Delegate authentication to a different application.</Description>
    <PackageProjectUrl>https://github.com/xTRUMANx/DelegatedAuthentication</PackageProjectUrl>
    <RepositoryUrl>https://github.com/xTRUMANx/DelegatedAuthentication</RepositoryUrl>
    <Version>2.3.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.Abstractions" Version="2.3.0" />
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.3.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="9.0.2" />
  </ItemGroup>

</Project>
